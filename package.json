{
  "name": "etudiant-entrepreneur",
  "version": "0.0.1",
  "description": "Faciiter l'accès au statut et à ses bénéfices",
  "license": "AGPL-3.0",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all --sequential clean-dist lint test",
    "build": "babel-node tools/build.js",
    "prebuild:gh-pages": "npm run prebuild",
    "build:gh-pages": "babel-node tools/build.js",
    "build:open": "npm-run-all --sequential build open:dist",
    "deploy": "npm run build:gh-pages && gh-pages -d dist",
    "test": "mocha tools/testSetup.js \"src/**/*.test.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.test.js\" --reporter progress",
    "test:cover:ci": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.test.js\" --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgmap/etudiant-entrepreneur.git"
  },
  "author": "Nicolas Brohée",
  "bugs": {
    "url": "https://github.com/sgmap/etudiant-entrepreneur/issues"
  },
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "bootstrap": "3.3.7",
    "jquery": "3.1.0",
    "react": "15.3.0",
    "react-bootstrap": "0.30.2",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-router-bootstrap": "0.23.1",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "toastr": "2.1.2",
    "validator": "5.5.0"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-es2015-native-modules": "6.9.3",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.13.0",
    "babel-register": "6.11.6",
    "browser-sync": "2.14.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.12",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.2.2",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-react": "6.0.0",
    "eslint-watch": "2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "0.9.0",
    "gh-pages": "0.11.0",
    "html-webpack-plugin": "2.22.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jsdom": "9.4.1",
    "mocha": "3.0.2",
    "nock": "8.0.0",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "open": "0.0.5",
    "postcss-loader": "0.9.1",
    "react-addons-test-utils": "15.3.0",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.1.2",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-md5-hash": "0.0.5"
  }
}

{
  "name": "etudiant-entrepreneur",
  "version": "0.0.1",
  "description": "Faciiter l'accès au statut et à ses bénéfices",
  "license": "AGPL-3.0",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm-run-all --sequential clean-dist lint test",
    "build": "babel-node tools/build.js",
    "prebuild:gh-pages": "npm run prebuild",
    "build:gh-pages": "babel-node tools/build.js",
    "build:open": "npm-run-all --sequential build open:dist",
    "deploy": "npm run build:gh-pages && gh-pages -d dist",
    "test": "mocha tools/testSetup.js \"src/**/*.test.js\" --reporter spec",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.test.js\" --reporter progress",
    "test:cover:ci": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.test.js\" --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgmap/etudiant-entrepreneur.git"
  },
  "author": "Nicolas Brohée",
  "bugs": {
    "url": "https://github.com/sgmap/etudiant-entrepreneur/issues"
  },
  "dependencies": {
    "axios": "0.15.3",
    "babel-polyfill": "6.22.0",
    "bootstrap": "3.3.7",
    "bootswatch": "3.3.7",
    "jquery": "3.1.1",
    "jwt-decode": "2.1.0",
    "moment": "2.17.1",
    "react": "15.4.2",
    "react-addons-shallow-compare": "15.4.2",
    "react-bootstrap": "0.30.7",
    "react-bootstrap-date-picker": "4.0.0",
    "react-dates": "5.2.0",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-bootstrap": "0.23.1",
    "react-router-redux": "4.0.7",
    "react-textarea-autosize": "4.0.5",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "toastr": "2.1.2",
    "validator": "6.2.1"
  },
  "devDependencies": {
    "autoprefixer": "6.7.0",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.22.0",
    "babel-plugin-transform-react-jsx": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.22.0",
    "babel-register": "6.22.0",
    "browser-sync": "2.18.6",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "enzyme": "2.7.1",
    "eslint": "3.14.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-watch": "2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "gh-pages": "0.12.0",
    "html-webpack-plugin": "2.26.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.5",
    "jsdom": "9.9.1",
    "mocha": "3.2.0",
